[run]
source = .
omit =
    */migrations/*
    */tests/*
    */settings/*
    */wsgi.py
    */asgi.py
    manage.py
    */apps.py
    */urls.py
    */admin.py
    */__init__.py
    */venv/*
    */env/*
    */.virtualenvs/*

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    if settings.DEBUG
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError
    except ImportError:
